<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false" >
        <shape>
			<corners android:radius="3.5dp" />
			<solid android:color="@color/c12" />
			<stroke android:color="@color/c6" android:width="1px" />
		</shape>
    </item>
    <item android:state_pressed="true" >
        <shape>
			<corners android:radius="3.5dp" />
			<solid android:color="#166ba5" />
			<stroke android:color="@color/c1" android:width="1px"/>
		</shape>
    </item>
    <item>
        <shape>
			<corners android:radius="3.5dp" />
			<solid android:color="@color/c1" />
			<stroke android:color="@color/c8" android:width="1px"/>
		</shape>
    </item>
</selector>
